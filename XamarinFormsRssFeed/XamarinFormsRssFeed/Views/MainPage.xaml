             x:Class="XamarinFormsRssFeed.Views.MainPage"
             xmlns:local="clr-namespace:XamarinFormsRssFeed.ViewModels"
             Title="Agixis Feeder">
    <ContentPage.Resources>
        <ResourceDictionary>
            <local:MainPageViewModel x:Key="viewModel">
            </local:MainPageViewModel>
        </ResourceDictionary>
    </ContentPage.Resources>
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="ValidateButton"  Text="Ajouter" VerticalOptions="StartAndExpand" HorizontalOptions="EndAndExpand"/>
